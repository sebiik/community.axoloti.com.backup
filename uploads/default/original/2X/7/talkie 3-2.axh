	0x0B,0x17,0x22,0x2E,0x39,0x45,0x51,0x5C
};
const int8_t tmsK6[0x10]  = {
	0xC0,0xCB,0xD6,0xE1,0xEC,0xF7,0x03,0x0E,\
	0x19,0x24,0x2F,0x3A,0x45,0x50,0x5B,0x66
};
const int8_t tmsK7[0x10]  = {
	0xB3,0xBF,0xCB,0xD7,0xE3,0xEF,0xFB,0x07,\
	0x13,0x1F,0x2B,0x37,0x43,0x4F,0x5A,0x66
};
const int8_t tmsK8[0x08]  = {
	0xC0,0xD8,0xF0,0x07,0x1F,0x37,0x4F,0x66
};
const int8_t tmsK9[0x08]  = {
	0xC0,0xD4,0xE8,0xFC,0x10,0x25,0x39,0x4D
};
const int8_t tmsK10[0x08] = {
	0xCD,0xDF,0xF1,0x04,0x16,0x20,0x3B,0x4D
};

///////////////////////////
// Synth State Variables //
///////////////////////////

bool synthUnvoiced;
uint8_t synthPeriod;
uint16_t synthEnergy;
int16_t synthK1,synthK2;
int8_t synthK3,synthK4,synthK5,synthK6,synthK7,synthK8,synthK9,synthK10;

////////////////////////////////////
// Word Data Read-State Variables //
////////////////////////////////////

int wordLength;

uint8_t* ptrAddr;
uint8_t ptrBit;
uint8_t* ptrStart;

uint8_t* ptrGlitchOffset;
uint8_t ptrBitGlitchOffset;

////////////////////////////////////
// Read/Render Counters/Intervals //
////////////////////////////////////

uint16_t readCounter;
uint16_t readInterval;

///////////////////
// Params/Inlets //
///////////////////

bool trig;
uint32_t wordIndex;
uint32_t rate;
bool loop;
bool freeze;
bool glitch;
bool step;

///////////////////
///////////////////
//// FUNCTIONS ////
///////////////////
///////////////////

//////////////////////
// Set Data Pointer //
//////////////////////

//void tk_setPtr(uint8_t* addr, int len) {
void tk_setPtr(uint8_t* addr) {
	ptrAddr = addr;
	ptrStart = ptrAddr;
	ptrBit = 0;
	ptrGlitchOffset = 0;
	ptrBitGlitchOffset = 0;
	//wordLength = len;
}

///////////////////////////////////
// Reset Pointer To Sample Start //
///////////////////////////////////

void tk_resetToStart() {
	ptrAddr = ptrStart;
	ptrBit = 0;
}

////////////////////
// Glitch Pointer //
////////////////////

void tk_glitchPtr() {
	ptrGlitchOffset = ptrAddr;
	ptrBitGlitchOffset = ptrBit;
	ptrBit = GenerateRandomNumber() % 7;//>> 23;
}

///////////////////////////
// Add Offset To Pointer //
///////////////////////////

void tk_glitchPtrReset() {
	ptrAddr = ptrGlitchOffset;
	ptrBit = ptrBitGlitchOffset;
}

////////////////////
// Read Data Bits //
////////////////////

uint8_t tk_getBits(uint8_t bits) {
	uint8_t value;
	uint16_t data;
	data = tk_revBits(*(ptrAddr))<<8;
	if (ptrBit+bits > 8) {
		data |= tk_revBits(*(ptrAddr + 1));
	}
	data <<= ptrBit;
	value = data >> (16-bits);
	ptrBit += bits;
	if (ptrBit >= 8) {
		ptrBit -= 8;
		ptrAddr++;
	}
	return value;
}

//////////////////
// Reverse Bits //
//////////////////

uint8_t tk_revBits(uint8_t a) {
	// REDO with Cortex M4 builtin REV function?	
	// 76543210
	a = (a>>4) | (a<<4); // Swap in groups of 4
	// 32107654
	a = ((a & 0xcc)>>2) | ((a & 0x33)<<2); // Swap in groups of 2
	// 10325476
	a = ((a & 0xaa)>>1) | ((a & 0x55)<<1); // Swap bit pairs
	// 01234567
	return a;
}

/////////////////////////
// Silence Synthesizer //
/////////////////////////

void tk_silence() {
	synthPeriod = 0;
	synthEnergy = 0;
	synthK1  = 0;
	synthK2  = 0;
	synthK3  = 0;
	synthK4  = 0;
	synthK5  = 0;
	synthK6  = 0;
	synthK7  = 0;
	synthK8  = 0;
	synthK9  = 0;
	synthK10 = 0;
}

/////////////////
// Read Frames //
/////////////////

void tk_readWord() {
	uint8_t repeat;
	uint8_t energy;
	// Speak if end of word not reached and ptrAddress is set
	if (energy != 0xf && ptrAddr) {		
		energy = tk_getBits(4);
		if (energy == 0) {
			// Energy = 0: rest frame
			synthEnergy = 0;
		} else if (energy == 0xf) {
			// Energy = 15: stop frame. Silence the synthesiser.
			tk_silence();
			if(loop)
				tk_resetToStart();
		} else {
			synthEnergy = tmsEnergy[energy];
			repeat = tk_getBits(1);
			synthPeriod = tmsPeriod[tk_getBits(6)];
			// A repeat frame uses the last coefficients
			if (!repeat) {
				// All frames use the first 4 coefficients
				synthK1 = tmsK1[tk_getBits(5)];
				synthK2 = tmsK2[tk_getBits(5)];
				synthK3 = tmsK3[tk_getBits(4)];
				synthK4 = tmsK4[tk_getBits(4)];	
				if (synthPeriod) {
					synthUnvoiced = false;
					// Voiced frames use 6 extra coefficients.
					synthK5  = tmsK5[ tk_getBits(4)];
					synthK6  = tmsK6[ tk_getBits(4)];
					synthK7  = tmsK7[ tk_getBits(4)];
					synthK8  = tmsK8[ tk_getBits(3)];
					synthK9  = tmsK9[ tk_getBits(3)];
					synthK10 = tmsK10[tk_getBits(3)];
				} else {
					synthUnvoiced = true;
					// Zero above coefficients for unvoiced frames
					synthK5  = 0;
					synthK6  = 0;
					synthK7  = 0;
					synthK8  = 0;
					synthK9  = 0;
					synthK10 = 0;
				}
			}
		}
	}
}

///////////////////////
// Utility Functions //
///////////////////////

// Scale input to range out_min > out_max
__attribute__ ( ( always_inline ) ) __STATIC_INLINE int32_t range_pos(int32_t val, int32_t out_min, int32_t out_max) {
	float x = (float)val;
	float outMin = (float)out_min;
	float outMax = (float)out_max;
	return (int32_t)(x * (outMax - outMin) / 0x07FFFFFF + outMin);
};

__attribute__ ( ( always_inline ) ) __STATIC_INLINE int8_t u8mix(int8_t a, int8_t b, uint8_t x) {
	return x * b + (255 - x) * a >> 8;
}

// Exponential interpolation
__attribute__ ( ( always_inline ) ) __STATIC_INLINE int32_t interpLog(int32_t val) {
	// Transform inlet val to float in 0 - 1 range
	float inf = val * (float)(1.0f / (1 << 27));
	float curve = 1 - (1 - inf) * (1 - inf);
	return (int32_t)(curve * (float)(1 << 27));
}

]]></code.declaration>
