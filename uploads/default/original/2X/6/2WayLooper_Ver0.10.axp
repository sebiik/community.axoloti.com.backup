         <patchobj type="patch/object" uuid="38f43da1-e7ec-41c5-9d29-3f8116545db1" name="Rot_1" x="784" y="770">
            <params>
               <int32 name="value" value="0"/>
               <int32 name="min" value="0"/>
               <int32 name="max" value="64"/>
               <int32 name="preset" value="32"/>
            </params>
            <attribs/>
            <object id="patch/object" uuid="38f43da1-e7ec-41c5-9d29-3f8116545db1">
               <sDescription>Reads a rotary encoder
trig - trigger input on rising edge
dir - dir&gt;0=clockwise, dir&lt;0=anti-clockwise</sDescription>
               <author>Gavin</author>
               <license>BSD</license>
               <helpPatch>logic.axh</helpPatch>
               <inlets>
                  <int32 name="value"/>
                  <bool32.rising name="trig"/>
                  <bool32.rising name="dir" description="decrement trigger"/>
                  <bool32 name="mom"/>
                  <bool32.rising name="enable"/>
               </inlets>
               <outlets>
                  <int32 name="o" description="output"/>
                  <bool32 name="b"/>
                  <bool32 name="a"/>
               </outlets>
               <displays/>
               <params>
                  <int32 name="value">
                     <MinValue i="-10000"/>
                     <MaxValue i="10000"/>
                  </int32>
                  <int32 name="min">
                     <MinValue i="-10000"/>
                     <MaxValue i="10000"/>
                  </int32>
                  <int32 name="max">
                     <MinValue i="-10000"/>
                     <MaxValue i="10000"/>
                  </int32>
                  <int32 name="preset">
                     <MinValue i="-10000"/>
                     <MaxValue i="10000"/>
                  </int32>
               </params>
               <attribs/>
               <includes/>
               <code.declaration><![CDATA[int val; 
 int encoderPos;
 int encoderLast;
 int n;
