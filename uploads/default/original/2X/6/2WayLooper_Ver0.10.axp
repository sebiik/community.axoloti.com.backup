      <object id="patch/object" uuid="331519fb-b6a9-411a-af1d-fbdcf18e738e">
         <sDescription>Reads the boolean inputs as 8-bit binary, and outputs the corresponding int (0-255)</sDescription>
         <author>Jeppe Rasmussen</author>
         <license>BSD</license>
         <helpPatch>bin_to_int.axh</helpPatch>
         <inlets>
            <bool32 name="i0"/>
            <bool32 name="i1"/>
            <bool32 name="i2"/>
            <bool32 name="i3"/>
            <bool32 name="i4"/>
         </inlets>
         <outlets>
            <int32.positive name="o0"/>
         </outlets>
         <displays/>
         <params/>
         <attribs/>
         <includes/>
         <code.declaration><![CDATA[int result;]]></code.declaration>
         <code.krate><![CDATA[result = 0;
if (inlet_i0 != 0) {result = result + 0;}
if (inlet_i1 != 0) {result = result + 1;}
if (inlet_i2 != 0) {result = result + 2;}
if (inlet_i3 != 0) {result = result + 3;}
if (inlet_i4 != 0) {result = result + 4;}
outlet_o0 = result;]]></code.krate>
      </object>
   </patchobj>
