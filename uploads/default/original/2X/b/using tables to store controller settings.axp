   </obj>
   <comment type="patch/comment" x="1330" y="1162" text="click here to save the preset to the sd card"/>
   <obj type="ctrl/button" uuid="ef3d13774349df85aa9bfecf30dbc91ab8488b3f" name="button_3" x="1386" y="1176">
