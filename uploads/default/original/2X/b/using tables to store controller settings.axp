      <attribs/>
   </obj>
   <comment type="patch/comment" x="28" y="504" text="This saves the table to a file on the sdcard, in this case the same file the table is referencing. (which means it will be loaded at startup)"/>
   <comment type="patch/comment" x="714" y="504" text="This object is normally for copying tables."/>
   <comment type="patch/comment" x="28" y="518" text="It will interrupt the audio and cause a glitch, so don&apos;t use it mid-jam,"/>
   <obj type="string/c" uuid="4aa90a90c435a742ddfa152d232883fc5b2f1b3" name="c2" x="378" y="518">
      <params/>
      <attribs>
         <table attributeName="str" table="/datasave"/>
      </attribs>
   </obj>
   <comment type="patch/comment" x="714" y="518" text="But in this case it will clear the table (not the file saved on sd card)."/>
   <comment type="patch/comment" x="28" y="532" text="just press it before you turn off the axoloti. "/>
   <comment type="patch/comment" x="714" y="532" text="Can be useful for re-setting all controls to 0"/>
   <obj type="rbrt/data/tcopy" uuid="d4c37687-69fd-442f-b560-b32c25224396" name="tcopy_1" x="812" y="546">
      <params>
         <int32 name="src" value="0"/>
         <int32 name="dst" value="0"/>
         <int32 name="range" value="1024"/>
         <bool32.tgl name="splat" value="0"/>
         <bool32.tgl name="clear" value="1"/>
      </params>
      <attribs>
         <objref attributeName="src" obj="data"/>
         <objref attributeName="dst" obj="data"/>
         <spinner attributeName="speed" value="1"/>
      </attribs>
   </obj>
   <obj type="table/save" uuid="3e4108b607d56d9e1cd10abb898b58eea11e53b2" name="c" x="378" y="588">
