<patch-1.0 appVersion="1.0.12">
   <comment type="patch/comment" x="28" y="14" text="This is the table called data, referencing file &quot;datasave&quot; on sdcard."/>
   <comment type="patch/comment" x="28" y="28" text="the names can be anything you want"/>
   <obj type="table/alloc 16b sdram load" uuid="e6b85dad3ab43565cda93ea4c43c73b668b1cd43" name="data" x="28" y="42">
      <params/>
      <attribs>
         <combo attributeName="size" selection="1024"/>
         <file attributeName="filename" file="/datasave"/>
      </attribs>
   </obj>
   <comment type="patch/comment" x="322" y="168" text="this writes the data from the controller to the table"/>
   <obj type="ctrl/dial p" uuid="cc5d2846c3d50e425f450c4b9851371b54f4d674" name="dial_1" x="28" y="182">
      <params>
         <frac32.u.map name="value" value="0.0"/>
      </params>
      <attribs/>
   </obj>
   <comment type="patch/comment" x="1204" y="182" text="To add more than 8 controls, simply add another object."/>
   <obj type="rbrt/data/tablestore 8" uuid="736a5df0-09ec-472a-a4ba-fde5617e1001" name="tablestore_1" x="364" y="196">
      <params/>
      <attribs>
         <objref attributeName="table" obj="data"/>
      </attribs>
   </obj>
   <obj type="disp/dial p" uuid="44fd18b562e434b3230441681132dbeabb15cdc5" name="dial_2" x="742" y="196">
      <params/>
      <attribs/>
   </obj>
   <comment type="patch/comment" x="1204" y="196" text="But to make sure it doesn&apos;t write the data into the same place in the table as the previous object, put a ctrl/i to offset it."/>
   <comment type="patch/comment" x="196" y="210" text="Plug your controllers in here -&gt;"/>
   <comment type="patch/comment" x="490" y="210" text="&lt;- Plug these into whatever you want to control"/>
   <obj type="ctrl/dial p" uuid="cc5d2846c3d50e425f450c4b9851371b54f4d674" name="dial_3" x="112" y="224">
      <params>
         <frac32.u.map name="value" value="0.0"/>
      </params>
      <attribs/>
   </obj>
   <comment type="patch/comment" x="196" y="224" text="For midi use midi/in objects."/>
   <obj type="disp/dial p" uuid="44fd18b562e434b3230441681132dbeabb15cdc5" name="dial_4" x="812" y="224">
      <params/>
      <attribs/>
   </obj>
   <obj type="ctrl/i" uuid="a3786816db6ea5bc6ac4193a5cccdb2c83b83496" name="i_1" x="1218" y="224">
      <params>
         <int32 name="value" value="8"/>
      </params>
      <attribs/>
   </obj>
   <comment type="patch/comment" x="1274" y="224" text="the offset here means the table will write the 8 controllers plugged into it to bits 8 - 15 of the table"/>
   <comment type="patch/comment" x="1274" y="238" text=" the first tablestore object had no offset so is writing to bits 0-7 (i&apos;m not sure bits is the right terminology here, but whatever)"/>
   <obj type="ctrl/toggle" uuid="42b8134fa729d54bfc8d62d6ef3fa99498c1de99" name="toggle_2" x="196" y="252">
