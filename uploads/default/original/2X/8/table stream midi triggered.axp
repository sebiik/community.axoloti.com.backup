   <patchobj type="patch/object" uuid="43d5c0c3-ca7a-491d-ba99-9d7e5b926a74" name="counter2_1" x="686" y="42">
      <params>
         <int32 name="banks" value="4"/>
         <int32 name="variations" value="8"/>
      </params>
      <attribs/>
      <object id="patch/object" uuid="43d5c0c3-ca7a-491d-ba99-9d7e5b926a74">
         <sDescription>cyclic up/down counter</sDescription>
         <author>Johannes Taelman</author>
         <license>BSD</license>
         <helpPatch>logic.axh</helpPatch>
         <inlets>
            <bool32.rising name="load" description="increment trigger"/>
         </inlets>
         <outlets>
            <int32 name="bank"/>
            <int32 name="variation" description="output"/>
            <bool32.pulse name="c" description="carry pulse"/>
            <bool32 name="finished"/>
         </outlets>
         <displays>
            <bool32 name="loading"/>
         </displays>
         <params>
            <int32 name="banks">
               <MinValue i="1"/>
               <MaxValue i="64"/>
            </int32>
            <int32 name="variations">
               <MinValue i="1"/>
               <MaxValue i="64"/>
            </int32>
         </params>
         <attribs/>
         <includes/>
         <code.declaration><![CDATA[int trig;
   int count;
int DO=1;]]></code.declaration>
         <code.init><![CDATA[count=0;
   trig = 0;]]></code.init>
         <code.krate><![CDATA[outlet_c=0;
if(((inlet_load>0)&&!trig)&&DO){
	trig=1;
	outlet_c=1;
	int tmp=param_variations*param_banks;
	int cnt1=count/param_variations;
	outlet_variation=count-cnt1*param_variations;
	outlet_bank=cnt1;
	count+=1;
	if(count==tmp){DO=0;}
}
else if(inlet_load<1){trig=0;}
outlet_finished=!DO;
disp_loading=DO;]]></code.krate>
      </object>
   </patchobj>
