         <code.init><![CDATA[ccsel=1;
ccboard=0;]]></code.init>
         <code.krate><![CDATA[disp_layer=ccsel;
disp_board=ccboard;]]></code.krate>
         <code.midihandler><![CDATA[if ((status == attr_selchan-1 + MIDI_CONTROL_CHANGE)&&(data1 == 1))
{
	ccboard = data2;
}
if ((status == attr_selchan-1 + MIDI_CONTROL_CHANGE)&&(data1 == 2))
{
	ccsel = data2;
}

if ((status == attr_selchan-1 + MIDI_CONTROL_CHANGE)&&(data1 >= 2))
{
	switch(ccboard){
case 0: MidiSend3((midi_device_t) attr_device1, MIDI_CONTROL_CHANGE +channel[ccboard],data1,data2);break;
case 1: MidiSend3((midi_device_t) attr_device2, MIDI_CONTROL_CHANGE +channel[ccboard],data1,data2);break;
case 2: MidiSend3((midi_device_t) attr_device3, MIDI_CONTROL_CHANGE +channel[ccboard],data1,data2);break;
case 3: MidiSend3((midi_device_t) attr_device4, MIDI_CONTROL_CHANGE +channel[ccboard],data1,data2);break;
case 4: MidiSend3((midi_device_t) attr_device5, MIDI_CONTROL_CHANGE +channel[ccboard],data1,data2);break;
case 5: MidiSend3((midi_device_t) attr_device6, MIDI_CONTROL_CHANGE +channel[ccboard],data1,data2);break;
}
}]]></code.midihandler>
