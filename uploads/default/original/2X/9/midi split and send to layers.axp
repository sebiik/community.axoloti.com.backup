         <spinner attributeName="pos" value="0"/>
         <spinner attributeName="max" value="64"/>
         <spinner attributeName="min" value="-64"/>
      </attribs>
      <object id="patch/object" uuid="285b579a-9ce5-4be8-8343-26bb47533655">
         <sDescription></sDescription>
         <inlets/>
         <outlets>
            <frac32 name="out"/>
         </outlets>
         <displays/>
         <params/>
         <attribs>
            <objref name="table"/>
            <spinner name="layer" MinValue="0" MaxValue="127" DefaultValue="0"/>
            <spinner name="pos" MinValue="0" MaxValue="45" DefaultValue="0"/>
            <spinner name="max" MinValue="-64" MaxValue="64" DefaultValue="64"/>
            <spinner name="min" MinValue="-64" MaxValue="64" DefaultValue="-64"/>
         </attribs>
         <includes/>
         <code.declaration><![CDATA[int diff=(attr_max-(attr_min)<<22);
int min=attr_min<<21;
int sel=attr_layer*46+attr_pos;]]></code.declaration>
         <code.krate><![CDATA[outlet_out=___SMMUL(attr_table.array[sel],diff)+min;]]></code.krate>
      </object>
