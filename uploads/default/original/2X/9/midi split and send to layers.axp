   <patchobj type="patch/object" uuid="a8580098-3d1a-41a0-98cc-eec378551e70" name="table_device_1" x="294" y="280">
      <params/>
      <attribs>
         <spinner attributeName="channel" value="1"/>
         <spinner attributeName="layers" value="128"/>
      </attribs>
      <object id="patch/object" uuid="a8580098-3d1a-41a0-98cc-eec378551e70">
         <sDescription>Receives Midi Continuous Controller messages</sDescription>
         <author>Johannes Taelman</author>
         <license>BSD</license>
         <helpPatch>midi_in.axh</helpPatch>
         <inlets/>
         <outlets/>
         <displays>
            <uint8array128.vbar name="d1"/>
         </displays>
         <params/>
         <attribs>
            <spinner name="channel" MinValue="1" MaxValue="16" DefaultValue="1"/>
            <spinner name="layers" description="sets amount of layers. there are 46 controls per layer." MinValue="1" MaxValue="128" DefaultValue="0"/>
         </attribs>
         <includes/>
         <code.declaration><![CDATA[static const uint32_t LENGTH=(46*attr_layers);
static const uint32_t BITS = 32;
static const uint32_t GAIN = 0;

int32_t ccv,CC;
uint32_t *array;
int layer,ntrig;]]></code.declaration>
         <code.init><![CDATA[static uint32_t _array[LENGTH]  __attribute__ ((section (".sdram")));
array = &_array[0];
layer=0;
CC=0;
ccv=0;

for(int i=0;i<LENGTH;i++){
	array[i]=0;
}]]></code.init>
         <code.krate><![CDATA[if(ntrig){
	ntrig=0;
	for(int i=0;i<46;i++){
		disp_d1[i]=array[i+layer]>>24;
	}
}]]></code.krate>
         <code.midihandler><![CDATA[if ((status == attr_channel-1 + MIDI_CONTROL_CHANGE)&&(data1>2))
{
	ntrig=1;
	CC=data1-3;
	ccv = data2;
	array[layer+CC]=(uint32_t)ccv<<24;
}

if ((status == attr_channel-1 + MIDI_CONTROL_CHANGE)&&(data1==2))
{
	ntrig=1;
layer=(data2>=attr_layers?attr_layers-1:data2)*46;
}]]></code.midihandler>
      </object>
   </patchobj>
