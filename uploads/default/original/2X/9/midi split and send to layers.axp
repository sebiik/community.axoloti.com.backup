   <patchobj type="patch/object" uuid="a8580098-3d1a-41a0-98cc-eec378551e70" name="preset/rnd morpher" x="588" y="770">
      <params/>
      <attribs>
         <objref attributeName="table" obj="table_device_1"/>
      </attribs>
      <object id="patch/object" uuid="a8580098-3d1a-41a0-98cc-eec378551e70">
         <sDescription>Receives Midi Continuous Controller messages</sDescription>
         <author>Johannes Taelman</author>
         <license>BSD</license>
         <helpPatch>midi_in.axh</helpPatch>
         <inlets>
            <bool32 name="copy"/>
            <bool32 name="paste"/>
            <bool32 name="rnd"/>
            <bool32 name="generate"/>
            <frac32 name="variate"/>
            <int32 name="preset"/>
            <bool32 name="update"/>
            <frac32 name="mix"/>
            <bool32 name="load"/>
            <bool32 name="save"/>
            <charptr32 name="filename"/>
         </inlets>
         <outlets/>
         <displays/>
         <params/>
         <attribs>
            <objref name="table"/>
         </attribs>
         <includes/>
         <code.declaration><![CDATA[uint32_t LENGTH;

uint32_t *array;
uint32_t *pst;
uint32_t *rnd;
int ntrig,copy,paste,gen,RND,var,load,save,upd,mix;]]></code.declaration>
         <code.init><![CDATA[static uint32_t _array[attr_table.LENGTH]  __attribute__ ((section (".sdram")));
array = &_array[0];
static uint32_t _rnd[attr_table.LENGTH]  __attribute__ ((section (".sdram")));
rnd = &_rnd[0];
static uint32_t _pst[attr_table.LENGTH]  __attribute__ ((section (".sdram")));
pst = &_pst[0];
LENGTH=attr_table.LENGTH;

for(int i=0;i<LENGTH;i++){
	rnd[i]=(uint32_t)(GenerateRandomNumber()>>1);
}]]></code.init>
         <code.krate><![CDATA[if(inlet_copy&&!copy){
	copy=1;
	for(int i=0;i<LENGTH;i++){
		array[i]=attr_table.array[i];
	}
}
else if(!inlet_copy){copy=0;}

if(inlet_paste&&!paste){
	paste=1;
	for(int i=0;i<LENGTH;i++){
		attr_table.array[i]=array[i];
	}
	attr_table.ntrig=1;
}
else if(!inlet_paste){paste=0;}

if(inlet_rnd&&!RND){
	RND=1;
	for(int i=0;i<LENGTH;i++){
		attr_table.array[i]=(uint32_t)(GenerateRandomNumber()>>1);
	}
	attr_table.ntrig=1;
}
else if(!inlet_rnd){RND=0;}

if(inlet_generate&&!gen){
	gen=1;
	for(int i=0;i<LENGTH;i++){
	rnd[i]=(uint32_t)(GenerateRandomNumber()>>1);
}
}
else if(!inlet_generate){gen=0;}

if(inlet_update&&!upd){
	upd=1;
	for(int i=0;i<LENGTH;i++){
	attr_table.array[i]=pst[i];
	attr_table.ntrig=1;
}
}
else if(!inlet_update){upd=0;}

if(!(inlet_variate==var)){
	for(int i=0;i<LENGTH;i++){
		uint32_t tmp1=array[i]>>1;
		uint32_t tmp2=rnd[i]>>1;
		attr_table.array[i]=tmp1+(___SMMUL(tmp2-tmp1,inlet_variate)<<5)<<1;
	}
	attr_table.ntrig=1;
}
var=inlet_variate;

if(!(inlet_mix==mix)){
	for(int i=0;i<LENGTH;i++){
		uint32_t tmp1=array[i]>>1;
		uint32_t tmp2=pst[i]>>1;
		attr_table.array[i]=tmp1+(___SMMUL(tmp2-tmp1,inlet_mix)<<5)<<1;
	}
	attr_table.ntrig=1;
}
mix=inlet_mix;



if ((inlet_load>0) && !load) {
    load=1;
    FIL FileObject;
    FRESULT err;
    UINT bytes_read;
    codec_clearbuffer();
    err = f_open(&FileObject, inlet_filename, FA_READ | FA_OPEN_EXISTING);
    if (err != FR_OK) { report_fatfs_error(err,inlet_filename); return;}
    int rem_sz = sizeof(*pst)*LENGTH;
    int offset = 0;
    while (rem_sz>0) {
      if (rem_sz>sizeof(fbuff)) {
        err = f_read(&FileObject, fbuff, sizeof(fbuff),&bytes_read);
        if (bytes_read == 0) break;
        memcpy((char *)(&pst[0]) + offset,(char *)fbuff,bytes_read);
        rem_sz -= bytes_read;
        offset += bytes_read;
      } else {
        err = f_read(&FileObject, fbuff, rem_sz,&bytes_read);
        memcpy((char *)(&pst[0]) + offset,(char *)fbuff,bytes_read);
        rem_sz = 0;
      }
    }    if (err != FR_OK) { report_fatfs_error(err,inlet_filename); return;};
    err = f_close(&FileObject);
    if (err != FR_OK) { report_fatfs_error(err,inlet_filename); return;};
  }
  else if (!(inlet_load>0)) {load=0;}

if ((inlet_save>0) && !save) {
    save=1;
    FIL FileObject;
    FRESULT err;
    UINT bytes_written;
    codec_clearbuffer();
    err = f_open(&FileObject, inlet_filename, FA_WRITE | FA_CREATE_ALWAYS);
    if (err != FR_OK) {report_fatfs_error(err,inlet_filename); return;}
    int rem_sz = sizeof(*attr_table.array)*attr_table.LENGTH;
    int offset = 0;
    while (rem_sz>0) {
      if (rem_sz>sizeof(fbuff)) {
        memcpy((char *)fbuff,(char *)(&attr_table.array[0]) + offset, sizeof(fbuff));
        err = f_write(&FileObject, fbuff, sizeof(fbuff),&bytes_written);
        rem_sz -= sizeof(fbuff);
        offset += sizeof(fbuff);
      } else {
        memcpy((char *)fbuff,(char *)(&attr_table.array[0]) + offset, rem_sz);
        err = f_write(&FileObject, fbuff, rem_sz, &bytes_written);
        rem_sz = 0;
      }
    }    if (err != FR_OK) report_fatfs_error(err,inlet_filename);
    err = f_close(&FileObject);
    if (err != FR_OK) report_fatfs_error(err,inlet_filename);
  }
  else if (!(inlet_save>0)) {save=0;}]]></code.krate>
      </object>
   </patchobj>
