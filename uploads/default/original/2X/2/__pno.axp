   <patchobj type="patch/object" uuid="ff7c538f-8976-4046-8241-d6a8ba39dcc5" name="sw2shift_note" x="462" y="770">
      <params/>
      <attribs>
         <text attributeName="script">
            <sText><![CDATA[//old cc mapping
/*
if (status == MIDI_CONTROL_CHANGE + attr_midichannel) {
	if ((data1 >= 23)&&(data1<31)) {
		if (data2 > 63) {
			uint8_t val = data1-23;
			out1 = val; //send integer value to control mux6
			o1 = led0[val];
			o2 = led1[val];
			o3 = led2[val];
		}
	}
	//else if (data1==29) {
	//	out2 = data2 >> 6; //send integer value between 0..1 to control mux2
	//}
}*/



if ((status == MIDI_NOTE_ON + _channel) && (data2)) {
	
		out2 = data1; //just output pitch data
		out1 = note2num[data1]; //and do it mapped now

}
]]></sText>
         </text>
      </attribs>
      <object id="patch/object" uuid="ff7c538f-8976-4046-8241-d6a8ba39dcc5">
         <sDescription>script with 2 outputs, triggered by MIDI input</sDescription>
         <author>Johannes Taelman</author>
         <license>BSD</license>
         <helpPatch>script.axh</helpPatch>
         <inlets/>
         <outlets>
            <int32 name="out1_" description="out1"/>
            <int32 name="out2_" description="out2"/>
            <bool32 name="led0"/>
            <bool32 name="led1"/>
            <bool32 name="led2"/>
         </outlets>
         <displays/>
         <params/>
         <attribs>
            <text name="script"/>
         </attribs>
         <includes/>
         <code.declaration><![CDATA[int32_t out1,out2;
bool o1, o2, o3;
int8_t _channel;


uint8_t note2num[128]; //contains lookup table for note assignment to number]]></code.declaration>
         <code.init><![CDATA[out1=0;
out2=0;
_channel = 1;

for (uint8_t i=0; i<128; i++) {
	note2num[i] = i;
}

//add the note to transposition mapping here!
//the array index equals the note number
//the assignment equals the index of the transposition
note2num[48] = 0;
note2num[50] = 1;
note2num[52] = 2;
note2num[53] = 3;
note2num[55] = 4;
note2num[57] = 5;
note2num[59] = 6;
note2num[60] = 7;
note2num[62] = 8;
note2num[64] = 9;]]></code.init>
         <code.krate><![CDATA[outlet_out1__ = this->out1;
outlet_out2__ = this->out2;
outlet_led0 = this->o1;
outlet_led1 = this->o2;
outlet_led2 = this->o3;]]></code.krate>
         <code.midihandler><![CDATA[attr_script]]></code.midihandler>
      </object>
   </patchobj>
