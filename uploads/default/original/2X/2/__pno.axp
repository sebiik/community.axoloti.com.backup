   <patchobj type="patch/object" uuid="ff7c538f-8976-4046-8241-d6a8ba39dcc5" name="sw2shift_note_" x="420" y="1106">
      <params/>
      <attribs>
         <text attributeName="script">
            <sText><![CDATA[//old cc mapping
/*
if (status == MIDI_CONTROL_CHANGE + attr_midichannel) {
	if ((data1 >= 23)&&(data1<31)) {
		if (data2 > 63) {
			uint8_t val = data1-23;
			out1 = val; //send integer value to control mux6
			o1 = led0[val];
			o2 = led1[val];
			o3 = led2[val];
		}
	}
	//else if (data1==29) {
	//	out2 = data2 >> 6; //send integer value between 0..1 to control mux2
	//}
}*/



if (((status == MIDI_NOTE_ON + _channel) && (data2))) {
	switch (data1) {
		case 0: o1 = 1; break;
		case 1: o2 = 1; break;
		case 5: o3 = 1; break;
		case 6: o4 = 1; break;
		case 7: o5 = 1; break;
		case 33: o6 = 1; break;
		case 34: o7 = 1; break;
		case 35: o8 = 1; break;
		case 36: o9 = 1; break;
		default: oDefault = 1; break;
	}
}
else if ((status == MIDI_NOTE_OFF + _channel) && (data2)){
	switch (data1) {
		case 0: o1 = 0; break;
		case 1: o2 = 0; break;
		case 5: o3 = 0; break;
		case 6: o4 = 0; break;
		case 7: o5 = 0; break;
		case 33: o6 = 0; break;
		case 34: o7 = 0; break;
		case 35: o8 = 0; break;
		case 36: o9 = 0; break;
		default: oDefault = 0; break;
	}
}

]]></sText>
         </text>
      </attribs>
