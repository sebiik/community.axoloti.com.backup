		}


	bendA=0;
	bendB=0;

	prevstep=synclock;
	}


//reset bend & touch
	if ((inlet_reset || (synclock<1))&&!resetrig){
		MidiSend3((midi_device_t) attr_output, MIDI_PITCH_BEND+ (attr_channel-1),0,64);
		
		resetrig=1;
		
		}

if (!inlet_reset && (synclock>0)){
	resetrig=0;
	}]]></code.krate>
