	synclock=inlet_synclock>>attr_resolution;
	}
else {synclock=-1;}

ObendA=attr_table.array[synclock+offset];
ObendB=attr_table.array[synclock+offset+voice];

if (inlet_clear&&!ctrig){
	clear=0;
	ctrig=1;
}
if (!inlet_clear){
	ctrig=0;
}
