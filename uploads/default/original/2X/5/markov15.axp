               <code.init><![CDATA[ntrig=0;]]></code.init>
               <code.krate><![CDATA[if ((inlet_trig>0) && !ntrig)
        {
          lastnote = (64+(inlet_note>>21))&0x7F;
          MidiSend3((midi_device_t) attr_device, MIDI_NOTE_ON + (attr_midichannel),lastnote,inlet_velo>>20);
