      <object id="patch/object" uuid="6e484072-4a54-4acb-9e3b-543afb3ba59d">
         <sDescription>A bipolar input, bipolar output k rate function defined by 2 segments.</sDescription>
         <author>Smashed Transistors</author>
         <license>LGPL</license>
         <helpPatch>b2b.axh</helpPatch>
         <inlets>
            <frac32.bipolar name="in"/>
            <frac32.bipolar name="p2"/>
            <bool32 name="allgrains"/>
         </inlets>
         <outlets>
            <frac32 name="out"/>
         </outlets>
         <displays/>
         <params>
            <frac32.s.map name="p0"/>
            <frac32.s.map name="p1"/>
            <frac32.s.map name="p2"/>
         </params>
         <attribs/>
         <includes/>
         <code.krate><![CDATA[int32_t inin;

if (!inlet_allgrains){
	inin=inlet_in;
}
else{
	inin=0x07FFFFFF;
}


int32_t in = (inin + (1<<27)) >> 1;

if(in >= (1<<27))
  outlet_out = param_p2+inlet_p2;
else if(in <= 0)
  outlet_out = param_p0;
else{
  int32_t i = in >> (27-1);              // i [0 2[
  int32_t a = (in & ((1<<(27-1)) - 1));  // a q26
  if(i < 1){ // [0 1[             q27         q26   q21           q27 
    outlet_out = ___SMMLA(param_p1 - param_p0, a, param_p0 >> 6) << 6;
  } else {   // [1 2[
    outlet_out = ___SMMLA((param_p2+inlet_p2) - param_p1, a, param_p1 >> 6) << 6;
  }
}]]></code.krate>
      </object>
