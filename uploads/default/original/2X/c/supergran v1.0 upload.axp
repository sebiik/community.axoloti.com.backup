if (ppqout!=prevppq){
	ppqgate=1;
	prevppq=ppqout;
}
ppqwidth=___SMMUL((Okbeat/ppq)<<3,inlet_ppqgate<<2);
if (ppqwidth<3){
	ppqwidth=3;
}

if (ppqgate>0){
	ppqgate+=1;
	if (ppqgate>=ppqwidth){
		ppqgate=0;
	}
}

 


outlet_ppq=ppqout;
outlet_ppqgate=ppqgate;


outlet_active = active;
outlet_start=Xstart[mode];
