            <object id="patch/object" uuid="5c2da8d2-3d43-4bde-a4a6-4cb42e57a743">
               <sDescription>read from table, nearest neighbour</sDescription>
               <author>Johannes Taelman</author>
               <license>BSD</license>
               <helpPatch>table.axh</helpPatch>
               <inlets>
                  <bool32 name="gate"/>
                  <frac32 name="in"/>
                  <int32 name="selectNote"/>
                  <frac32 name="offset"/>
                  <int32 name="octaves"/>
                  <frac32 name="delay"/>
                  <frac32 name="glide"/>
                  <frac32 name="exponential"/>
               </inlets>
               <outlets>
                  <frac32 name="o" description="table[a]"/>
               </outlets>
               <displays/>
               <params/>
               <attribs>
                  <objref name="table"/>
               </attribs>
               <includes/>
               <code.declaration><![CDATA[int trg;
int sel;
int32_t val,hld;
int32_t t;]]></code.declaration>
               <code.krate><![CDATA[int32_t o=inlet_in;

int32_t p,del;
MTOF(-inlet_glide,p)
MTOF(-inlet_delay,del)
int octaves=inlet_octaves>0?inlet_octaves*12:144;
int sl=inlet_selectNote>=0?inlet_selectNote:0;

sl=sl>9?9:sl;
   if(attr_table.active>1)
   {
   	if(inlet_gate&&!trg)
   	{
   		trg=1;
   		if((attr_table.vcnt<attr_table.cnt)||(attr_table.vcnt>(attr_table.cnt+1)))
   		{
   			attr_table.vcnt=(attr_table.vcnt+1)&7;
   		}
   		
   		val=(1<<31)-1;
   		hld=(1<<31)-1;
   	
   	sel=(attr_table.vcnt+(attr_table.active*100)-sl-1)%attr_table.active;
   	if(sl==8)
   	{
   		sel=(attr_table.vcnt-(GenerateRandomNumber()%(attr_table.active-1)))&7;
   	}
   	int r=o>>21;
   	
   	if(sl==9)
   	{
   		int min=500;
   		int Ps=0;
   		for(int i=0;i<attr_table.active;i++)
   		{
   			int dif=r-(attr_table.array[i]-64);
	   		dif=dif>0?dif:-dif;
	   		if((dif<min)&&(!(dif==0)))
	   		{
	   			min=dif;
	   			Ps=i;
	   		}
   		}
   		sel=Ps;
   	}
   	t=(attr_table.array[sel]-64);
   	
   	int32_t df=t-r;
   	if(df>0)
   	{
   		df=df%octaves;
   	}
   	else
   	{
   		df=-((-df)%(octaves));
   	}
   	t=r+df<<21;
   	}
   	else if(!inlet_gate){trg=0;}
   	
   	
   	o+=___SMMUL(t-o+inlet_offset,val)<<1;
   }
   int32_t dif=___SMMUL(val,p);
   dif=(p>>3)+(___SMMUL(dif-(p>>3),__USAT(inlet_exponential,27)<<4)<<1);
   hld=__USAT(hld-(del>>2),31);
   if(hld==0)
   {
   val=__USAT(val-dif,31);
   }
   outlet_o= o;]]></code.krate>
            </object>
