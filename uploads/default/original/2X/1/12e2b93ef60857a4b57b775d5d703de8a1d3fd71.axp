   <patchobj type="patch/object" uuid="9aff3aa8-67f5-493b-8c00-45b898e0e9f4" name="square_2" x="224" y="252">
      <params>
         <frac32.s.map name="pitch" value="-14.0"/>
         <frac32.s.map name="pw" value="-6.0"/>
         <frac32.s.map name="humanize" value="22.0"/>
      </params>
      <attribs/>
      <object id="patch/object" uuid="9aff3aa8-67f5-493b-8c00-45b898e0e9f4">
         <sDescription>square wave LFO, boolean output, frequency input</sDescription>
         <author>Johannes Taelman</author>
         <license>BSD</license>
         <helpPatch>lfo.axh</helpPatch>
         <inlets>
            <frac32.bipolar name="pitch" description="pitch"/>
            <bool32.rising name="reset" description="reset phase"/>
         </inlets>
         <outlets>
            <bool32 name="beat" description="square wave"/>
            <frac32 name="phs"/>
            <bool32 name="pulse"/>
         </outlets>
         <displays/>
         <params>
            <frac32.s.map.lfopitch name="pitch" noLabel="true"/>
            <frac32.s.map name="pw"/>
            <frac32.s.map name="humanize"/>
         </params>
         <attribs/>
         <includes/>
         <code.declaration><![CDATA[int32_t Phase;
uint32_t r;
int32_t pw;
int32_t pulse,rate;]]></code.declaration>
         <code.init><![CDATA[Phase = 0;
   r = 1;]]></code.init>
         <code.krate><![CDATA[int32_t prv=((Phase>>4)+pw)>0?1:-1;
int32_t pphs=Phase;
if(pulse>0)
{
	pulse-=rate;
}
else{pulse=0;}

	int32_t freq;
	MTOFEXTENDED(param_pitch + inlet_pitch,freq);
      if (inlet_reset && r) {
         Phase = 0;
         r = 0;
      } else {
         if (!inlet_reset) r = 1;
         
         
         Phase += freq>>2;
      }
      outlet_beat= (Phase>0)?1:0;
      outlet_phs=((uint32_t)Phase)>>5;
      int now=((Phase>>4)+pw)>0?1:-1;
      
      if(!(now==prv))
      {
      	pulse=(1<<29)-1;
      	
      	rate=freq+(GenerateRandomNumber()%((1<<31)-freq));
      }
      if((Phase<0)&&(pphs>0)){pw=___SMMUL(GenerateRandomNumber(),param_humanize>>2)+param_pw;}
      
      
      outlet_pulse=pulse>0?1:0;]]></code.krate>
      </object>
   </patchobj>
