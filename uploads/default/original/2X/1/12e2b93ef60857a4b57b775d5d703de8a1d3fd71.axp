         <patchobj type="patch/object" uuid="b83daf6f-b354-4838-bf9a-41a3bc114d5d" name="envelope" x="322" y="56">
            <params>
               <frac32.s.map name="a" onParent="true" value="0.0"/>
               <frac32.s.map name="d" onParent="true" value="0.0"/>
               <frac32.u.map name="s" onParent="true" value="0.0"/>
               <frac32.s.map name="r" onParent="true" value="0.0"/>
            </params>
            <attribs/>
            <object id="patch/object" uuid="b83daf6f-b354-4838-bf9a-41a3bc114d5d">
               <sDescription>Attack/decay/sustain/release envelope</sDescription>
               <author>Johannes Taelman</author>
               <license>BSD</license>
               <helpPatch>adsr.axh</helpPatch>
               <inlets>
                  <frac32 name="gate"/>
               </inlets>
               <outlets>
                  <frac32.positive name="env" description="envelope output"/>
               </outlets>
               <displays/>
               <params>
                  <frac32.s.map.klineartime.exp2 name="a"/>
                  <frac32.s.map.kdecaytime.exp name="d"/>
                  <frac32.u.map name="s"/>
                  <frac32.s.map.kdecaytime.exp name="r"/>
               </params>
               <attribs/>
               <includes/>
               <code.declaration><![CDATA[int8_t stage;
int ntrig;
int32_t val,lvl;]]></code.declaration>
               <code.init><![CDATA[stage = 0;
ntrig = 0;
val = 0;
lvl=0;]]></code.init>
               <code.krate><![CDATA[if ((inlet_gate>0) && !ntrig) {
   stage = 1;
   ntrig = 1;
   lvl=val+___SMMUL((1<<27)-1-val<<1,__USAT(inlet_gate,27)<<4);
}
if (!(inlet_gate>0) && ntrig) {
   stage = 0;
   ntrig=0;
}
if (stage == 0){
   val = ___SMMUL(val,param_r)<<1;
} else if (stage == 1){
   val = val + (___SMMUL(lvl,param_a)<<5);
   if ((val<0)||(val>lvl)) {
      val =lvl;
      stage = 2;
