		if(dir[sel]){pn=((1<<31)-1)-pn;}
		int32_t ll=lvl[sel];
		tm=___SMMUL(array[(writepos-tm)&LENGTHMASK]<<14,ll)<<1;
		ll=___SMMUL(tm,pn)<<1;
		l+=tm-ll;
		r+=ll;
		dir[sel]=!dir[sel];
	}
}
outlet_l=l+inlet_in;
outlet_r=r+inlet_in;]]></code.srate>
